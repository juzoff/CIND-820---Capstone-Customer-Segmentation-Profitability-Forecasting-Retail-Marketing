","print(\"\\nNote: Friedman Test indicates significant differences in total_transactions ranks across clusters within promotion types (Statistic=6.0000, p=0.049787, p < 0.05).\")\n","\n","# Save results to CSV\n","df.to_csv('cluster0_sales_revenue_promotion_types.csv', index=False)\n","print(\"\\nResults saved to 'cluster0_sales_revenue_promotion_types.csv'\")\n","\n","# Create the bar chart\n","plt.figure(figsize=(8, 6))\n","bars = plt.bar(df['promotion_type'], df['total_sales_revenue'],\n","               color=['#1f77b4', '#2ca02c', '#ff7f0e'])  # Blue, Green, Orange\n","\n","# Add labels on top of the bars with a dollar sign\n","for bar in bars:\n","    plt.text(\n","        bar.get_x() + bar.get_width()/2,  # Center x\n","        bar.get_height(),  # Top of bar\n","        f\"${bar.get_height():,.2f}\",  # Dollar sign, two decimals\n","        ha='center', va='bottom', fontsize=12, fontweight='bold'\n","    )\n","\n","# Add titles and labels\n","plt.title('Total Sales Revenue for Cluster 0 by Promotion Type', fontsize=14)\n","plt.xlabel('Promotion Type', fontsize=12)\n","plt.ylabel('Total Sales Revenue ($)', fontsize=12)\n","\n","# Rotate x-axis labels for better readability\n","plt.xticks(rotation=45, ha='right')\n","\n","# Save and show the plot\n","plt.savefig('cluster0_sales_revenue_by_promotion_type.png')\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":830},"id":"mtm7oPLq-Pr0","executionInfo":{"status":"ok","timestamp":1750198714351,"user_tz":240,"elapsed":535,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"378ef04c-bbb0-4c5f-d2bf-da093351e59f"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Total Sales Revenue for Cluster 0 by Promotion Type:\n","         promotion_type  total_sales_revenue\n","0               20% Off           13847.3700\n","1  Buy One Get One Free           14561.8300\n","2            Flash Sale           14031.1200\n","\n","Note: 