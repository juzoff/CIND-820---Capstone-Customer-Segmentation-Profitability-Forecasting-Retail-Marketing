umns])\n","    print(f\"Friedman Test: Statistic={stat:.4f}, p-value={p_value:.6f}\")\n","    if p_value < 0.05:\n","        print(\"Significant differences in total_transactions ranks across clusters within income brackets (p < 0.05).\")\n","        # Summary table for cluster-income_bracket transactions\n","        cluster_summary = train_data.groupby(['cluster', 'season'])['total_transactions'].mean().unstack()\n","        print(\"\\nseason by Cluster and Income Bracket:\")\n","        print(cluster_summary.round(2))\n","    else:\n","        print(\"No significant differences found (p â‰¥ 0.05).\")\n","except ValueError as e:\n","    print(f\"Error in Friedman test: {e}\")\n","\n","# Visualization: Box plot of total_transactions by income_bracket and cluster\n","plt.figure(figsize=(10, 6))\n","sns.boxplot(x='season', y='total_transactions', hue='cluster', data=train_data)\n","plt.title('Total Transactions by season and Cluster')\n","plt.xlabel('Income Bracket')\n","plt.ylabel('Total Transactions')\n","plt.legend(title='Cluster')\n","plt.tight_layout()\n","\n","# Save plot\n","plt.savefig('friedman_boxplot_income_bracket_transactions.png')\n","plt.show()\n","\n","# Save results to CSV\n","results = pd.DataFrame({\n","    'Test': ['Friedman'],\n","    'Statistic': [stat],\n","    'p_value': [p_value],\n","    'Significant': [p_value < 0.05]\n","})\n","results.to_csv('friedman_results_income_bracket_transactions.csv', index=False)\n","print(\"Results saved to 'friedman_results_income_bracket_transactions.csv'\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":781},"id":"ghjosNCz9a1o","executionInfo":{"status":"ok","timestamp":1750198936257,"user_tz":240,"elapsed":2018,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"98997733-1d66-4aa0-d584-386a2939a7d8"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Friedman Test: Statistic=8.0000, p-value=0.018316\n","Significant differences