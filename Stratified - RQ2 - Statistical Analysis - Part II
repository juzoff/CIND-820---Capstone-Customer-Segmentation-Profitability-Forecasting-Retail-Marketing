oss clusters within product_category"],"metadata":{"id":"95no2tlS7oqF"}},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","from scipy.stats import friedmanchisquare\n","import seaborn as sns\n","import matplotlib.pyplot as plt\n","\n","# Verify required columns in train_data\n","required_columns = ['total_discounts_received', 'product_category', 'cluster']\n","if not all(col in train_data.columns for col in required_columns):\n","    missing = [col for col in required_columns if col not in train_data.columns]\n","    raise ValueError(f\"Missing columns in train_data: {missing}\")\n","\n","# Handle missing values\n","if train_data[required_columns].isnull().any().any():\n","    print(\"Warning: Missing values detected. Dropping rows with NaN.\")\n","    train_data = train_data.dropna(subset=required_columns)\n","\n","# Pivot data for Friedman test: rows as product_category, columns as clusters\n","pivot_data = train_data.pivot_table(\n","    values='total_discounts_received',\n","    index='product_category',\n","    columns='cluster',\n","    aggfunc='mean'  # Average discounts per cluster-category pair\n",")\n","\n","# Check if pivot_data has at least 3 clusters for Friedman test\n","if pivot_data.shape[1] < 3:\n","    raise ValueError(f\"Friedman test requires at least 3 clusters, found {pivot_data.shape[1]}\")\n","\n","# Perform Friedman test\n","try:\n","    stat, p_value = friedmanchisquare(*[pivot_data[col] for col in pivot_data.columns])\n","    print(f\"Friedman Test: Statistic={stat:.4f}, p-value={p_value:.6f}\")\n","    if p_value < 0.05:\n","        print(\"Significant differences in total_discounts_received ranks across clusters within product categories (p < 0.05).\")\n","        # Summary table for cluster-category discounts\n","        cluster_summary = train_data.groupby(['cluster', 'product_category'])['total_discounts_received'].mean().unstack()\n","        print(\"\\nAverage Total Discounts by Cluster and Product Category:\")\n","        print(cluster_summary.round(2))\n","    else:\n","        print(\"No significant differences found (p â‰¥ 0.05).\")\n","except ValueError as e:\n","    print(f\"Error in Friedman test: {e}\")\n","\n","# Visualization: Box plot of total_discounts_received by product_category and cluster\n","plt.figure(figsize=(10, 6))\n","sns.boxplot(x='product_category', y='total_discounts_received', hue='cluster', data=train_data)\n","plt.title('Total Discounts Received by Product Category and Cluster')\n","plt.xlabel('Product Category')\n","plt.ylabel('Total Discounts Received')\n","plt.legend(title='Cluster')\n","plt.tight_layout()\n","\n","# Save plot\n","plt.savefig('friedman_boxplot_product_category_discounts.png')\n","plt.show()\n","\n","# Save results to CSV\n","results = pd.DataFrame({\n","    'Test': ['Friedman'],\n","    'Statistic': [stat],\n","    'p_value': [p_value],\n","    'Significant': [p_value < 0.05]\n","})\n","results.to_csv('friedman_results_discounts.csv', index=False)\n","print(\"Results saved to 'friedman_results_discounts.csv'\")"],"metadata":{"id":"Wujq9qDf5WSK","colab":{"base_uri":"https://localhost:8080/","height":781},"executionInfo":{"status":"ok","timestamp":1750198472409,"user_tz":240,"elapsed":886,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"b09082b0-f457-4be9-86b4-580610706b65"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Friedman Test: Statistic=10.0000, p-value=0.006738\n","Significant differences in total_discounts_received ranks across clusters within product categories (p < 0.05).\n","\n","Average Total Discounts by Cluster and Product Category:\n","product_category  Clothing  Electronics  Furniture  Groceries     Toys\n","cluster                                                               \n","0                 490.4400     500.9400   509.5300   508.2800 496.3500\n","1                 513.7500     518.2500   520.0200   529.2200 513.2800\n","2                 477.2500     480.9800   465.4400   497.2700 477.0900\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Results saved to 'friedman_results_discounts.csv'\n"]}]},{"cell_type":"markdown","source":["## Friedman Test - Significant differences in total_returned_value ranks across clusters within product_category"],"metadata":{"id":"7M9h2XrP7uf7"}},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","from scipy.stats import friedmanchisquare\n","import seaborn as sns\n","import matplotlib.pyplot as plt\n","\n","# Verify required columns in train_data\n","required_columns = ['total_returned_value', 'product_category', 'cluster']\n","if not all(col in train_data.columns for col in required_columns):\n","    missing = [col for col in required_columns if col not in train_data.columns]\n","    raise ValueError(f\"Missing columns in train_data: {missing}\")\n","\n","# Handle missing values\n","if train_data[required_columns].isnull().any().any():\n","    print(\"Warning: Missing values detected. Dropping rows with NaN.\")\n","    train_data = train_data.dropna(subset=required_columns)\n","\n","# Pivot data for Friedman test: rows as product_category, columns as clusters\n","pivot_data = train_data.pivot_table(\n","    values='total_returned_value',\n","    index='product_category',\n","    columns='cluster',\n","    aggfunc='mean'  # Average returned value per cluster-category pair\n",")\n","\n","# Check if pivot_data has at least 3 clusters for Friedman test\n","if pivot_data.shape[1] < 3:\n","    raise ValueError(f\"Friedman test requires at least 3 clusters, found {pivot_data.shape[1]}\")\n","\n","# Perform Friedman test\n","try:\n","    stat, p_value = friedmanchisquare(*[pivot_data[col] for col in pivot_data.columns])\n","    print(f\"Friedman Test: Statistic={stat:.4f}, p-value={p_value:.6f}\")\n","    if p_value < 0.05:\n","        print(\"Significant differences in total_returned_value ranks across clusters within product categories (p < 0.05).\")\n","        # Summary table for cluster-category returned value\n","        cluster_summary = train_data.groupby(['cluster', 'product_category'])['total_returned_value'].mean().unstack()\n","        print(\"\\nTotal Returned Value by Cluster and Product Category:\")\n","        print(cluster_summary.round(2))\n","    else:\n","        print(\"No significant differences found (p â‰¥ 0.05).\")\n","except ValueError as e:\n","    print(f\"Error in Friedman test: {e}\")\n","\n","# Visualization: Box plot of total_returned_value by product_category and cluster\n","plt.figure(figsize=(10, 6))\n","sns.boxplot(x='product_category', y='total_returned_value', hue='cluster', data=train_data)\n","plt.title('Total Returned Value by Product Category and Cluster')\n","plt.xlabel('Product Category')\n","plt.ylabel('Total Returned Value')\n","plt.legend(title='Cluster')\n","plt.tight_layout()\n","\n","# Save plot\n","plt.savefig('friedman_boxplot_product_category_returns.png')\n","plt.show()\n","\n","# Save results to CSV\n","results = pd.DataFrame({\n","    'Test': ['Friedman'],\n","    'Statistic': [stat],\n","    'p_value': [p_value],\n","    'Significant': [p_value < 0.05]\n","})\n","results.to_csv('friedman_results_returns.csv', index=False)\n","print(\"Results saved to 'friedman_results_returns.csv'\")"],"metadata":{"id":"-oWr9qS55Wnp"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["## Visualization - Net Revenue for Cluster 0 by Product Category"],"metadata":{"id":"Wzq7F6rj7zrd"}},{"cell_type":"code","source":["import pandas as pd\n","import matplotlib.pyplot as plt\n","\n","# Define data for Cluster 0 across product categories\n","data = {\n","    'product_category': ['Clothing', 'Electronics', 'Furniture', 'Groceries', 'Toys'],\n","    'total_transactions': [57.7500, 56.8000, 57.0600, 57.7800, 57.5300],\n","    'avg_transaction_value': [266.5200, 259.1900, 267.1900, 267.3500, 264.2600],\n","    'total_discounts_received': [490.4400, 500.9400, 509.5300, 508.2800, 496.3500],\n","    'total_returned_value': [551.4400, 538.9300, 542.1600, 544.1600, 538.9700]\n","}\n","\n","# Create DataFrame\n","df = pd.DataFrame(data)\n","\n","# Calculate total sales revenue\n","df['total_sales_revenue'] = (\n","    (df['avg_transaction_value'] * df['total_transactions'] - df['total_returned_value'] - df['total_discounts_received'])\n",")\n","\n","# Round total sales revenue to 2 decimal places for display\n","df['total_sales_revenue'] = df['total_sales_revenue'].round(2)\n","\n","# Print results\n","print(\"Total Sales Revenue for Cluster 0 by Product Category:\")\n","print(df[['product_category', 'total_sales_revenue']])\n","\n","# Save results to CSV\n","df.to_csv('cluster0_sales_revenue_product_categories.csv', index=False)\n","print(\"\\nResults saved to 'cluster0_sales_revenue_product_categories.csv'\")\n","\n","# Create the bar chart\n","plt.figure(figsize=(10, 6))  # Slightly wider to accommodate 5 bars\n","bars = plt.bar(df['product_category'], df['total_sales_revenue'],\n","               color=['#1f77b4', '#2ca02c', '#ff7f0e', '#d62728', '#9467bd'])  # Blue, Green, Orange, Red, Purple\n","\n","# Add labels on top of the bars with a dollar sign\n","for bar in bars:\n","    plt.text(\n","        bar.get_x() + bar.get_width()/2,  # Center x\n","        bar.get_height(),  # Top of bar\n","        f\"${bar.get_height():,.2f}\",  # Dollar sign, two decimals\n","        ha='center', va='bottom', fontsize=10, fontweight='bold'\n","    )\n","\n","# Add titles and labels\n","plt.title('Total Sales Revenue for Cluster 0 by Product Category', fontsize=14)\n","plt.xlabel('Product Category', fontsize=12)\n","plt.ylabel('Total Sales Revenue ($)', fontsize=12)\n","\n","# Rotate x-axis labels for better readability\n","plt.xticks(rotation=45, ha='right')\n","\n","# Save and show the plot\n","plt.savefig('cluster0_sales_revenue_by_product_category.png')\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":774},"id":"mTN-RBfr73s2","executionInfo":{"status":"ok","timestamp":1750198102174,"user_tz":240,"elapsed":455,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"6aa6e23d-30b3-4704-ccb9-007493e25b2d"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Total Sales Revenue for Cluster 0 by Product Category:\n","  product_category  total_sales_revenue\n","0         Clothing           14349.6500\n","1      Electronics           13682.1200\n","2        Furniture           14194.1700\n","3        Groceries           14395.0400\n","4             Toys           14167.5600\n","\n","Results saved to 'cluster0_sales_revenue_product_categories.csv'\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["# promotion_type"],"metadata":{"id":"AoCi0aqk87CX"}},{"cell_type":"markdown","source":["## Friedman Test - Significant differences in total_transactions ranks across clusters within promotion_type"],"metadata":{"id":"GzdChx6ye_sp"}},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","from scipy.stats import friedmanchisquare\n","import seaborn as sns\n","import matplotlib.pyplot as plt\n","\n","# Verify required columns in train_data\n","required_columns = ['total_transactions', 'promotion_type', 'cluster']\n","if not all(col in train_data.columns for col in required_columns):\n","    missing = [col for col in required_columns if col not in train_data.columns]\n","    raise ValueError(f\"Missing columns in train_data: {missing}\")\n","\n","# Handle missing values\n","if train_data[required_columns].isnull().any().any():\n","    print(\"Warning: Missing values detected. Dropping rows with NaN.\")\n","    train_data = train_data.dropna(subset=required_columns)\n","\n","# Pivot data for Friedman test: rows as promotion_type, columns as clusters\n","pivot_data = train_data.pivot_table(\n","    values='total_transactions',\n","    index='promotion_type',\n","    columns='cluster',\n","    aggfunc='mean'  # Average transactions per cluster-promotion_type pair\n",")\n","\n","# Check if pivot_data has at least 3 clusters for Friedman test\n","if pivot_data.shape[1] < 3:\n","    raise ValueError(f\"Friedman test requires at least 3 clusters, found {pivot_data.shape[1]}\")\n","\n","# Perform Friedman test\n","try:\n","    stat, p_value = friedmanchisquare(*[pivot_data[col] for col in pivot_data.columns])\n","    print(f\"Friedman Test: Statistic={stat:.4f}, p-value={p_value:.6f}\")\n","    if p_value < 0.05:\n","        print(\"Significant differences in total_transactions ranks across clusters within promotion types (p < 0.05).\")\n","        # Summary table for cluster-promotion_type transactions\n","        cluster_summary = train_data.groupby(['cluster', 'promotion_type'])['total_transactions'].mean().unstack()\n","        print(\"\\nAverage Total Transactions by Cluster and Promotion Type:\")\n","        print(cluster_summary.round(2))\n","    else:\n","        print(\"No significant differences found (p â‰¥ 0.05).\")\n","except ValueError as e:\n","    print(f\"Error in Friedman test: {e}\")\n","\n","# Visualization: Box plot of total_transactions by promotion_type and cluster\n","plt.figure(figsize=(10, 6))\n","sns.boxplot(x='promotion_type', y='total_transactions', hue='cluster', data=train_data)\n","plt.title('Total Transactions by Promotion Type and Cluster')\n","plt.xlabel('Promotion Type')\n","plt.ylabel('Total Transactions')\n","plt.legend(title='Cluster')\n","plt.tight_layout()\n","\n","# Save plot\n","plt.savefig('friedman_boxplot_promotion_type_transactions.png')\n","plt.show()\n","\n","# Save results to CSV\n","results = pd.DataFrame({\n","    'Test': ['Friedman'],\n","    'Statistic': [stat],\n","    'p_value': [p_value],\n","    'Significant': [p_value < 0.05]\n","})\n","results.to_csv('friedman_results_promotion_type_transactions.csv', index=False)\n","print(\"Results saved to 'friedman_results_promotion_type_transactions.csv'\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":781},"id":"HGwXhYX-8Tr8","executionInfo":{"status":"ok","timestamp":1750198390339,"user_tz":240,"elapsed":1112,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"a83de571-3720-4597-da2e-bb9e492f7d19"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Friedman Test: Statistic=6.0000, p-value=0.049787\n","Significant differences in total_transactions ranks across clusters within promotion types (p < 0.05).\n","\n","Average Total Transactions by Cluster and Promotion Type:\n","promotion_type  20% Off  Buy One Get One Free  Flash Sale\n","cluster                                                  \n","0               56.1800               58.3400     57.5700\n","1               43.4100               43.6800     43.0400\n","2               49.4300               48.7000     49.1300\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Results saved to 'friedman_results_promotion_type_transactions.csv'\n"]}]},{"cell_type":"markdown","source":["## Friedman Test - Significant differences in avg_transaction_value ranks across clusters within promotion_type"],"metadata":{"id":"nm9MayWAfJgg"}},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","from scipy.stats import friedmanchisquare\n","import seaborn as sns\n","import matplotlib.pyplot as plt\n","\n","# Verify required columns in train_data\n","required_columns = ['avg_transaction_value', 'promotion_type', 'cluster']\n","if not all(col in train_data.columns for col in required_columns):\n","    missing = [col for col in required_columns if col not in train_data.columns]\n","    raise ValueError(f\"Missing columns in train_data: {missing}\")\n","\n","# Handle missing values\n","if train_data[required_columns].isnull().any().any():\n","    print(\"Warning: Missing values detected. Dropping rows with NaN.\")\n","    train_data = train_data.dropna(subset=required_columns)\n","\n","# Pivot data for Friedman test: rows as promotion_type, columns as clusters\n","pivot_data = train_data.pivot_table(\n","    values='avg_transaction_value',\n","    index='promotion_type',\n","    columns='cluster',\n","    aggfunc='mean'  # Average transaction value per cluster-promotion_type pair\n",")\n","\n","# Check if pivot_data has at least 3 clusters for Friedman test\n","if pivot_data.shape[1] < 3:\n","    raise ValueError(f\"Friedman test requires at least 3 clusters, found {pivot_data.shape[1]}\")\n","\n","# Perform Friedman test\n","try:\n","    stat, p_value = friedmanchisquare(*[pivot_data[col] for col in pivot_data.columns])\n","    print(f\"Friedman Test: Statistic={stat:.4f}, p-value={p_value:.6f}\")\n","    if p_value < 0.05:\n","        print(\"Significant differences in avg_transaction_value ranks across clusters within promotion types (p < 0.05).\")\n","        # Summary table for cluster-promotion_type value\n","        cluster_summary = train_data.groupby(['cluster', 'promotion_type'])['avg_transaction_value'].mean().unstack()\n","        print(\"\\nAverage Transaction Value by Cluster and Promotion Type:\")\n","        print(cluster_summary.round(2))\n","    else:\n","        print(\"No significant differences found (p â‰¥ 0.05).\")\n","except ValueError as e:\n","    print(f\"Error in Friedman test: {e}\")\n","\n","# Visualization: Box plot of avg_transaction_value by promotion_type and cluster\n","plt.figure(figsize=(10, 6))\n","sns.boxplot(x='promotion_type', y='avg_transaction_value', hue='cluster', data=train_data)\n","plt.title('Average Transaction Value by Promotion Type and Cluster')\n","plt.xlabel('Promotion Type')\n","plt.ylabel('Average Transaction Value')\n","plt.legend(title='Cluster')\n","plt.tight_layout()\n","\n","# Save plot\n","plt.savefig('friedman_boxplot_promotion_type_avg_transaction.png')\n","plt.show()\n","\n","# Save results to CSV\n","results = pd.DataFrame({\n","    'Test': ['Friedman'],\n","    'Statistic': [stat],\n","    'p_value': [p_value],\n","    'Significant': [p_value < 0.05]\n","})\n","results.to_csv('friedman_results_promotion_type_avg_transaction.csv', index=False)\n","print(\"Results saved to 'friedman_results_promotion_type_avg_transaction.csv'\")\n","\n","# Additional descriptive statistics\n","cluster_summary = train_data.groupby(['cluster', 'promotion_type'])['avg_transaction_value'].agg(['mean']).unstack()\n","print(\"\\nDescriptive Statistics of Avg Transaction Value by Cluster and Promotion Type:\")\n","print(cluster_summary.round(2))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":798},"id":"Q5okxZ8N9D8y","executionInfo":{"status":"ok","timestamp":1750198405589,"user_tz":240,"elapsed":1244,"user":{"displayName":"Jivko Uzoff","userId":"13489242111202320155"}},"outputId":"1a12b9bd-3afa-4ac0-f9f3-63c12f032a85"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Friedman Test: Statistic=4.6667, p-value=0.096972\n","No significant differences found (p â‰¥ 0.05).\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Results saved to 'friedman_results_promotion_type_avg_transaction.csv'\n","\n","Descriptive Statistics of Avg Transaction Value by Cluster and Promotion Type:\n","                   mean                                \n","promotion_type  20% Off Buy One Get One Free Flash Sale\n","cluster                                                \n","0              265.0700             267.3900   261.9700\n","1              2